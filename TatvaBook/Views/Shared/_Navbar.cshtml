</head>

<header>
    <div class="container-fluid">
        <nav class="navbar navbar-expand-lg navbar-light">
            @*<img class="imagelogo" src="~/Images/logo.png" alt="logo">*@<h2 class="mx-2 Title">TatvaBook</h2>
            <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarTogglerDemo03" aria-controls="navbarTogglerDemo03" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>

            <div class="container-fluid">
                <form class="d-flex m-auto mx-4" id="navbarTogglerDemo03" role="search">
                    <input class="form-control me-2" type="search" placeholder="Search" aria-label="Search">
                    <button class="btn btn-outline-warning" type="submit">Search</button>
                </form>
            </div>


            <div class="ms-auto collapse navbar-collapse" id="navbarTogglerDemo03">
                <!-- Your content here -->
                <div class="ms-auto">
                    <ul class="navbar-nav">
                        <li class="nav-item">
                            <a class="nav-link" href="#"> <i class="bi bi-house-check mx-2"></i></a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="#"><i class="bi bi-people-fill mx-2"></i></a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="#"><i class="bi bi-chat-text-fill mx-2"></i></a>
                        </li>
                        <li class="nav-item">
                            <form method="post" asp-action="Logout" asp-controller="Login">
                                <button type="submit" class="nav-link btn btn-link py-0"
                                        style="width:auto">
                                    Logout @User.Identity.Name
                                </button>
                            </form>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>
    </div>

</header>

<body>
    <div class="container-fluid">

        <div class="row">
            <div class="pt-5 col-sm-2 col-md-3 mt-3 d-none d-sm-block sidebar">
                <div class="pt-3 ps-4 sidebarcontent d-none d-md-inline">
                    NAVIGATION
                </div>
                <div class="mt-3 atMediuemPaddingWholeContent">
                    <dl>
                        <dt class="mt-4 ">
                            <a asp-action="User" asp-controller="Admin" asp-route-SearchInputdata="" asp-route-pageindex=1 asp-route-pageSize=10 class="nav-link p-0 sidebarcontent d-flex atMediumConvertIntoColum user1">
                                <i class="bi bi-person-fill me-2 sidebar-img"></i> <span class="d-none d-md-inline sidebar-text-font">Home</span>
                            </a>
                        </dt>
                        <dt class="mt-4 ">
                            <a asp-action="Mission" asp-controller="Admin" asp-route-SearchInputdata="" asp-route-pageindex=1 asp-route-pageSize=10 class="nav-link p-0 sidebarcontent d-flex atMediumConvertIntoColum mission">
